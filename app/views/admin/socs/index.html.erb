<article class="hardware">
  <header>
    <h2>SoCs</h2>
  </header>

  <p><%= link_to 'Add another SoC', new_admin_soc_path, class: "btn btn-primary" %></p>

  <table class="table table-sm">
    <thead>
    <tr>
      <th>Model</th>
      <th>Load address</th>
      <th>Downloads</th>
      <th>Stage</th>
    </tr>
    </thead>
    <tbody>
      <%= render @socs %>
    </tbody>
  </table>
</article>
